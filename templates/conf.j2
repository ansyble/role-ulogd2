[global]
logfile="syslog"
loglevel={{ ulogd2_loglevel }}

plugin="{{ ulogd2_lib_dir }}/ulogd_inppkt_NFLOG.so"
plugin="{{ ulogd2_lib_dir }}/ulogd_inpflow_NFCT.so"
plugin="{{ ulogd2_lib_dir }}/ulogd_filter_IFINDEX.so"
plugin="{{ ulogd2_lib_dir }}/ulogd_filter_IP2STR.so"
plugin="{{ ulogd2_lib_dir }}/ulogd_filter_IP2BIN.so"
plugin="{{ ulogd2_lib_dir }}/ulogd_filter_PRINTPKT.so"
plugin="{{ ulogd2_lib_dir }}/ulogd_filter_HWHDR.so"
plugin="{{ ulogd2_lib_dir }}/ulogd_filter_PRINTFLOW.so"
plugin="{{ ulogd2_lib_dir }}/ulogd_output_LOGEMU.so"
plugin="{{ ulogd2_lib_dir }}/ulogd_output_SYSLOG.so"
plugin="{{ ulogd2_lib_dir }}/ulogd_output_XML.so"
plugin="{{ ulogd2_lib_dir }}/ulogd_output_GPRINT.so"
plugin="{{ ulogd2_lib_dir }}/ulogd_raw2packet_BASE.so"
plugin="{{ ulogd2_lib_dir }}/ulogd_inpflow_NFACCT.so"
plugin="{{ ulogd2_lib_dir }}/ulogd_output_GRAPHITE.so"
plugin="{{ ulogd2_lib_dir }}/ulogd_output_JSON.so"

stack=log1:NFLOG,base1:BASE,ifi1:IFINDEX,ip2str1:IP2STR,mac2str1:HWHDR,json1:JSON

[log1]
group=0

[log2]
group=1
numeric_label=1

[json1]
sync=1
device="{{ ulogd2_json_device_name }}"
boolean_label=1
file="{{ ulogd2_json_file }}"
